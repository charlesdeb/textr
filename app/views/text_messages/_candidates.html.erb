<% if @candidates.empty?%>
  <p>Sorry, no suggestions for that input</p>
<% else %>
  <ul>
    <% @candidates.each do |candidate| %>
      <li class='candidate'><%= candidate[:token_text] %> &nbsp;(count: <%= candidate[:count]  %>, chunk size: <%= candidate[:chunk_size]%>)</li>
    <% end %>
  </ul>
<% end %>
